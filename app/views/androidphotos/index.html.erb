
    <section id="portfolio" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h1 class="section-heading">Android</h1>
                </div>
            </div>
            <% @androidphotos.each do |androidphoto| %>
            
                <div class="col-md-6">

                        <div class="img-responsive" alt="">
                            
                        </div>
                        <a href="<%= androidphoto.linkpath %>"><%= image_tag(androidphoto.binary_data, size: "550x200") %></a>
                        <h5><%= androidphoto.description %></h5>
                </div>
                
            <% end %>
        </div>
    </section>
    
